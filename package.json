{
	"name": "mousetouch",
	"version": "2.0.0",
	"description": "A small library to provide consistent mouse and touch interactions.",
	"homepage": "https://github.com/obsius/mousetouch",
	"author": "Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)",
	"license": "MIT",
	"main": "lib/index.js",
	"scripts": {
		"prepare": "rollup --bundleConfigAsCjs -c"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"chrome": "100"
					}
				}
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.20.2",
		"@rollup/plugin-babel": "^6.0.3",
		"rollup": "^3.17.3"
	},
	"keywords": [
		"mouse",
		"touch",
		"events",
		"interaction"
	]
}
