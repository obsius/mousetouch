{
	"name": "mousetouch",
	"version": "1.2.0",
	"description": "A small library to provide consistent mouse and touch interactions.",
	"homepage": "https://github.com/obsius/mousetouch",
	"author": "Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)",
	"license": "MIT",
	"main": "lib/index.js",
	"scripts": {
		"prepare": "rollup -c"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"chrome": "80"
					}
				}
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"rollup": "^2.21.0",
		"rollup-plugin-babel": "^4.4.0"
	},
	"dependencies": {},
	"keywords": [
		"mouse",
		"touch",
		"events",
		"interaction"
	]
}
