{
	"name": "mousetouch",
	"version": "1.0.1",
	"description": "A small library to provide consistent mouse and touch interactions.",
	"author": "Jeff Seaman",
	"contributors": [
		"Jeff Seaman <mr.jeffrey.seaman@gmail.com> (https://www.ngc6822.com)"
	],
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"prepare": "babel src -d lib"
	},
	"babel": {
		"plugins": [
			"transform-runtime",
			"transform-decorators-legacy",
			[
				"babel-plugin-transform-builtin-extend",
				{
					"globals": [
						"Error",
						"Array"
					]
				}
			],
			[
				"babel-plugin-root-import",
				{
					"rootPathSuffix": "src/",
					"rootPathPrefix": "~"
				}
			]
		],
		"presets": [
			"env",
			"stage-2"
		]
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-root-import": "^6.1.0",
		"babel-plugin-transform-builtin-extend": "^1.1.2",
		"babel-plugin-transform-decorators-legacy": "^1.3.5",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-2": "^6.24.1",
		"copyfiles": "^2.1.0"
	},
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "https://github.com/obsius/mousetouch.git"
	},
	"keywords": [
		"mouse",
		"touch",
		"events",
		"interaction"
	]
}
